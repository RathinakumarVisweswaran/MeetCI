
<XESSPlus>
<classification>
  <datafile>Iris.csv</datafile>
  <input>4</input>
  <output>1</output>
  <classes>3</classes>
  <split>70</split>
  <delimiter>,</delimiter>
  <algorithm>
    <!--You have a CHOICE of the next 2 items at this level-->
    <MultiLayerPerceptron>
      <inputLayerActivation>Linear</inputLayerActivation>
      <hiddenLayerActivation>Sigmoid</hiddenLayerActivation>
      <hiddenLayers>3,4</hiddenLayers>
      <outputLayerActivation>Sigmoid</outputLayerActivation>
      <momentum>0.01</momentum>
      <epochs>30</epochs>
      <learningRate>0.01</learningRate>
    </MultiLayerPerceptron>
  </algorithm>
</classification>
</XESSPlus>
